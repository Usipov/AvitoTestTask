GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/BlocksKit" "${PODS_ROOT}/Headers/Public/Reachability"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/BlocksKit" -isystem "${PODS_ROOT}/Headers/Public/Reachability"
OTHER_LDFLAGS = $(inherited) -ObjC -l"BlocksKit" -l"Reachability" -framework "MessageUI" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods